let
  Source = Table.Combine({#"Fact PL OP Final Plan", #"Fact PL TL Final Plan", #"PL Combined Plan 2024"}),

  #"Filtered out APCAR and Inforce Retention" = Table.SelectRows(Source, each 
    ([Account] <> "APCAR" and [Account] <> "Inforce Retention")
  ),

  #"Filtered out SA & D0GRM" = Table.SelectRows(#"Filtered out APCAR and Inforce Retention", each 
    ([Distribution] <> "D0GRM") and  
    not Text.Contains([Distribution], "IA (Vol) State Auto")
  ),

  #"Changed column type" = Table.TransformColumnTypes(#"Filtered out SA & D0GRM", {{"Book Date", type date}})
  
in
  #"Changed column type"
