let
  Source = Excel.Workbook(File.Contents("\\PDC-AIP-DATA\ETS_ESSBASE_GCM_NAS_T3_PROD01\Power BI Reporting\Topline\State Cohorts.xlsx"), null, true),
  Navigation = Source{[Item = "LOB", Kind = "Sheet"]}[Data],
  #"Promoted headers" = Table.PromoteHeaders(Navigation, [PromoteAllScalars = true]),
  #"Filtered rows" = Table.SelectRows(#"Promoted headers", each [REF LOB] <> null and [REF LOB] <> ""),
  #"Removed columns" = Table.RemoveColumns(#"Filtered rows", {"Column3"}),
  #"Transform columns" = Table.TransformColumnTypes(#"Removed columns", {{"REF LOB", type text}, {"LOB", type text}}),
  #"Replace errors" = Table.ReplaceErrorValues(#"Transform columns", {{"REF LOB", null}, {"LOB", null}})
in
  #"Replace errors"
