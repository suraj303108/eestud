let
    Source = SharePoint.Files("https://libertymutual.sharepoint.com/sites/GRMFSDataStrategy", [ApiVersion = 15]),
    #"Sorted Rows" = Table.Sort(Source,{{"Date modified", Order.Descending}}),
  Navigation = #"Sorted Rows"{[Name = "2024 Combined Plan.csv", #"Folder Path" = "https://libertymutual.sharepoint.com/sites/GRMFSDataStrategy/Shared Documents/General/Final Plan/"]}[Content],
  #"Imported CSV" = Csv.Document(Navigation, [Delimiter = ",", Columns = 11, QuoteStyle = QuoteStyle.None]),
  #"Promoted headers" = Table.PromoteHeaders(#"Imported CSV", [PromoteAllScalars = true]),
  #"Changed column type" = Table.TransformColumnTypes(#"Promoted headers", {{"Account", type text}, {"Line", type text}, {"Book Date", type datetime}, {"Amount", type number}, {"State", type text}, {"Distribution", type text}, {"Reinsurance", type text}, {"", type text}, {"_1", type text}, {"_2", type text}, {"_3", type text}}),
  #"Removed other columns" = Table.SelectColumns(#"Changed column type", {"Account", "Line", "Book Date", "Amount", "State", "Distribution", "Reinsurance"}),
  #"Duplicated Distribution" = Table.DuplicateColumn(#"Removed other columns", "Distribution", "Distribution - Copy"),
  #"Split Column by Delimiter" = Table.SplitColumn(#"Duplicated Distribution", "Distribution - Copy", Splitter.SplitTextByDelimiter("-", QuoteStyle.Csv), {"Segment", "Channel"}),
  #"Renaming BME Summary Numerator" = Table.ReplaceValue(#"Split Column by Delimiter", "BME Summary Numerator", "BME Numerator", Replacer.ReplaceText, {"Account"}),
  #"Renaming BME Summary Denominator" = Table.ReplaceValue(#"Renaming BME Summary Numerator", "BME Summary Denominator", "BME Denominator", Replacer.ReplaceText, {"Account"}),
  #"Filtered Out PL" = Table.SelectRows(#"Renaming BME Summary Denominator", each Text.StartsWith([Distribution], "Mobility Pod") or Text.StartsWith([Distribution], "Custom") or Text.StartsWith([Distribution], "Energy")or Text.StartsWith([Distribution], "Express") or Text.StartsWith([Distribution], "Farm") or Text.StartsWith([Distribution], "Inland Marine"))
in
    #"Filtered Out PL"
