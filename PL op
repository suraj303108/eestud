import pandas as pd

# Load the historical and cy CSV files
historical_df = pd.read_csv('historical.csv')
cy_df = pd.read_csv('cy.csv')

# Append cy_df to historical_df
combined_df = historical_df.append(cy_df, ignore_index=True)

# Save the result back to a new CSV file (optional)
combined_df.to_csv('combined_historical.csv', index=False)

print("Files have been combined successfully!")
